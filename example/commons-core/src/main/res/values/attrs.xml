<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallPulse" value="0"/>
        </attr>
        <attr name="indicator_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="shape">
        <attr name="shape">
            <enum name="line" value="0"/>
            <enum name="oval" value="1"/>
            <enum name="rectangle" value="2"/>
            <enum name="ring" value="3"/>
        </attr>
        <attr name="corners_radius" format="float"/>
        <attr name="corners_bottomLeftRadius" format="float"/>
        <attr name="corners_bottomRightRadius" format="float"/>
        <attr name="corners_topLeftRadius" format="float"/>
        <attr name="corners_topRightRadius" format="float"/>
        <attr name="solid_color" format="reference|color"/>
        <attr name="stroke_color" format="reference|color"/>
        <attr name="stroke_width" format="reference|dimension"/>

    </declare-styleable>
    <declare-styleable name="actionbar">
        <attr name="actionBarTitleStytle" format="reference"/>
        <attr name="leftContainerStytle" format="reference"/>
        <attr name="rightContainerStytle" format="reference"/>
        <attr name="actionBarStytle" format="reference"/>
        <attr name="actionBarRightBtnStytle" format="reference"/>
        <attr name="actionBarLeftBtnStytle" format="reference"/>
    </declare-styleable>
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension|reference" />
        <attr name="border_inside_color" format="color|reference" />
        <attr name="border_outside_color" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="supper">
        <attr name="keepTop" format="boolean"/>
        <attr name="keepBottom" format="boolean"/>
        <attr name="keepLeft" format="boolean"/>
        <attr name="keepRight" format="boolean"/>
    </declare-styleable>
</resources>